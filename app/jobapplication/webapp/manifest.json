{
  "_version": "1.65.0",
  "sap.app": {
    "id": "jobapplication",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:feop",
      "version": "1.17.4",
      "toolsId": "ff56c62b-6f0f-4a81-9df4-cc2ea0f5c8ad"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/job-application/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "jobapplication-display": {
          "semanticObject": "jobapplication",
          "action": "display",
          "title": "{{jobapplication-display.flpTitle}}",
          "subTitle": "{{jobapplication-display.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.135.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "jobapplication.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": "Applicants({key}):?query:",
          "name": "ApplicantsObjectPage",
          "target": "ApplicantsObjectPage"
        }
      ],
      "targets": {
        "ApplicantsObjectPage": {
          "type": "Component",
          "id": "ApplicantsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": true,
              "contextPath": "/Applicants",
              "content": {
                "header": {
                  "visible": true,
                  "anchorBarVisible": true
                }
              },
              "controlConfiguration": {
                "education/@com.sap.vocabularies.UI.v1.LineItem#i18nEducationalSteps": {
                  "tableSettings": {
                    "widthIncludingColumnHeader": true,
                    "selectionMode": "Auto",
                    "type": "GridTable",
                    "personalization": {
                      "sort": false,
                      "filter": false,
                      "column": false
                    },
                    "enableExport": false,
                    "enablePaste": false,
                    "disableCopyToClipboard": false
                  },
                  "columns": {
                    "DataField::description": {
                      "width": "auto"
                    }
                  }
                },
                "career/@com.sap.vocabularies.UI.v1.LineItem#i18nCareerStages": {
                  "columns": {
                    "DataField::description": {
                      "width": "auto"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.fe.templates.ObjectPage.ObjectPageController": {
            "controllerName": "jobapplication.ext.controller.ObjectPage"
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
